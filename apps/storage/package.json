{
  "name": "storage",
  "version": "1.0.0",
  "main": "dist/apps/storage/src/main.js",
  "private": "true",
  "scripts": {
    "start": "ts-node-dev -r tsconfig-paths/register --ignore-watch  --respawn src/main.ts",
    "start:prod": "node dist/apps/storage/src/main.js",
    "build": "tsc -p tsconfig.app.json",
    "test": "nx jest:test",
    "test:watch": "nx jest:test --watch",
    "test:cov": "nx jest:test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "nx jest:test --config ./apps/backend/test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/s3-request-presigner": "^3.873.0",
    "@keyv/redis": "^5.1.2",
    "@nestjs/cache-manager": "^3.0.1",
    "cache-manager": "^7.2.0",
    "keyv": "^5.5.2",
    "nowhere-common": "workspace:*",
    "proto": "workspace:*",
    "redis": "^5.8.2"
  },
  "author": "Ali Hasan",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1"
}
