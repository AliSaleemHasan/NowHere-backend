{
  "name": "proto",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "proto:compile": "protoc --plugin=$(pnpm dlx which protoc-gen-ts_proto) --ts_proto_out=./ --ts_proto_opt=nestJs=true,outputServices=grpc-js,exportCommonSymbols=false *.proto -I .",
    "proto:build": "tsc -p tsconfig.lib.json",
    "proto:entry": "tsc scripts/generate-entry-file.ts && node scripts/generate-entry-file.js",
    "proto:gen": "pnpm proto:compile && pnpm proto:build && pnpm proto:entry",
    "proto:copy": "cp *.proto dist",
    "build": "pnpm proto:gen && rm -rf dist && tsc -p tsconfig.lib.json && pnpm proto:copy"
  },
  "dependencies": {
    "ts-proto": "^2.7.7"
  },
  "author": "Ali Hasan",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@bufbuild/protobuf": "^2.7.0"
  }
}
